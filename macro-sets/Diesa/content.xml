    <command>/me 
[h: AttackList = &quot;Attack|Basic Melee Attack,Cleave,Precise Strike,Reaping Strike,Shield Bash,Tempest Dance|Attack|LIST|VALUE=STRING&quot;]
[h: TargetList = &quot;Target|&quot; + targetList(&quot;npc&quot;) +&quot;|Target|RADIO|VALUE=STRING&quot;]
[h: abort(input(AttackList, TargetList))]

[h: WeaponDamage = 3]
[h: Bonus = if(Attack == &quot;Shield Bash&quot;, 2, WeaponDamage) + halfLevel()]
[h, if (Attack == &quot;Precise Strike&quot;): Bonus = Bonus + 4]
[h: Result = singleAttack(&quot;Strength&quot;, &quot;AC&quot;, Bonus, Target)]
[h: Description = json.get(Result, &quot;Description&quot;)]
[h: Roll = json.get(Result, &quot;Roll&quot;)]
[h: Target = json.get(Result, &quot;Target&quot;)]

[st, gt, t(Description): Roll] {Target} with {Attack}</command>
